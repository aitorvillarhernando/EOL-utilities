{
  "name": "EOL utilities",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Pack of utilities for the elotrolado.net website",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "action": {
    "default_icon": "icons/icon19.png",
    "default_title": "EOL utilities",
    "default_popup": "src/action/action.html"
  },
  "content_scripts": [{
    "matches": [
      "*://*.elotrolado.net/*"
    ],
    "js": [
      "js/cuenta-posts-eol.js"
    ]
  }]
}